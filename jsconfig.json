{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es6",
    "module": "commonjs",
    "jsx": "preserve",
    "checkJs": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "paths": {
      "@kared/*": ["./src/*"],
      "@mui/*": ["./node_modules/@mui/*"]
    }
  },
  "exclude": ["node_modules", "dist"],
  "include": ["src/**/*"]
}
